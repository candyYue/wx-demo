  ],
  "subpackages": [
    {
      "root": "subpackages/home/",
      "pages": [
        "home"
      ]
    },
    {
      "root": "subpackages/foot/",
      "pages": [
        "foot"
      ]
    },
    {
      "root": "subpackages/mydate/",
      "pages": [
        "mydate/mydate",
        "adddate/adddate"
      ]
    },
